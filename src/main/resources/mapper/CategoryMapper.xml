<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zbin.cisp.dao.CategoryMapper">

  <select id="getAllCategory" resultType="com.zbin.cisp.domain.Category">
    select * from category;
  </select>

  <insert id="create" useGeneratedKeys="true" keyProperty="id">
    insert into category(name,create_time) values (#{name},now())
  </insert>

  <delete id="deleteById">
    delete from category where id = #{id}
  </delete>

  <update id="updateById" parameterType="com.zbin.cisp.domain.Category">
    update category set name = #{name}
    where id = #{id}
  </update>
</mapper>